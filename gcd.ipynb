   "source": [
    "def gcd(a, b):\n",
    "    a, b = sorted([a, b])\n",
    "  \n",
    "    while True:\n",
    "        r = b % a\n",
    "        if r == 0:\n",
    "            break\n",
    "        a, b = r, a\n",
    "        \n",
    "    return a"
   ]
